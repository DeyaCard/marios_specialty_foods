<div class="container">
  <h1>Products</h1>

  <% if @products.any? %>
    <ul>
      <% @products.each do |product| %>
        <li><%= link_to product.name, product_path(product) %></li>
      <% end %>
  

  <% end %>
</div>
<p><%= link_to "Add new product", new_product_path%></p>
<p><%= link_to "Return to main menu", "/" %></p>

          

            

 


